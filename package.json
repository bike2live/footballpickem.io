{
  "name": "football-pickem2019",
  "version": "1.3.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open=true ",
    "start dev": "ng serve --open=true --proxy-config=proxy.conf.json",
    "startSSL": "ng serve --open=true --ssl true --ssl-key /private/etc/apache2/server.key --ssl-cert /private/etc/apache2/server.crt",
    "build": "ng build",
    "build-watch": "ng build --watch",
    "build-dev": "ng build --configuration=dev --stats-json",
    "build-dev-Watch": "ng build --configuration=dev --watch",
    "deploy-local": "cp -r ./dist/FootballPickem6/* ~/Sites/",
    "package": "ng build --prod --stats-json --optimization=false",
    "package-full": "ng build --prod",
    "test": "ng test ",
    "fullTest": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node scripts/version.js",
    "cypress:open": "cypress open"
  },
  "private": true,
  "dependencies": {
